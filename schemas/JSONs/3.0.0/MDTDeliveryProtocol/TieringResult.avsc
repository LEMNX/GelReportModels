            "fields" : [ {
              "name" : "versionControl",
              "type" : "VersionControl",
              "doc" : "Model version number"
            }, {
              "name" : "pedigreeId",
              "type" : "int",
              "doc" : "Numbering used to refer to each member of the pedigree"
