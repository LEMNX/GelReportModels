              "type" : "string"
            }, {
              "name" : "panelName",
              "type" : "string"
            }, {
              "name" : "panelVersion",
              "type" : [ "null", "string" ]
            }, {
              "name" : "review_outcome",
              "type" : "string"
            }, {
              "name" : "multiple_genetic_origins",
              "type" : "string"
            } ]
          }
        } ]
      }, {
